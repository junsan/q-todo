<?xml version='1.0' encoding='utf-8'?>
<widget id="org.cordova.quasar.app" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Q-Todo</name>
    <description>A Quasar Project</description>
    <author email="dev@cordova.apache.org" href="https://cordova.apache.org">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="http://qtodo.store/*" />
    <allow-intent href="*" />
    <allow-navigation href="about:*" />
    <allow-navigation href="http://qtodo.store/*" />
    <allow-navigation href="*" />
    <access origin="http://qtodo.store" />
    <access origin="http://qtodo.store/*" />
    <access origin="http://qtodo.store/qtodo-api/public/api" />
    <access origin="*" />
    <preference name="AndroidInsecureFileModeEnabled" value="true" />
</widget>
